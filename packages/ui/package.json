{
  "name": "ion-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-less-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "storybook": "npm-run-all -p watch-css storybook-actual",
    "storybook-actual": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build": "npm-run-all build-css build-js",
    "build-js": "react-scripts build"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^1.0.7",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/system": "^3.0.0-alpha.2",
    "animate.css": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "less": "^3.9.0",
    "lodash": "^4.17.11",
    "node-less-chokidar": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-scripts": "^2.1.1",
    "reset-css": "^4.0.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-links": "^4.1.1",
    "@storybook/react": "^4.1.1",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "font-awesome": "^4.7.0",
    "less-loader": "^4.1.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.17"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
